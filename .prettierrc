{
  "trailingComma": "all",
  "semi": true,
  "singleQuote": true,
  "printWidth": 100,
  "plugins": ["@pandabox/prettier-plugin", "@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^next\/?",
    "<THIRD_PARTY_MODULES>",
    "",
    "app\/",
    "",
    "services\/",
    "",
    "components\/",
    "",
    "^[./]",
    "",
    "^(?!.*[.]css$)[./].*$",
    ".css$"
  ],
  "importOrderParserPlugins": ["typescript", "jsx"],
  "importOrderTypeScriptVersion": "5.0.0",
  "importOrderCaseSensitive": false
}
